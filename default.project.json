{
	"name": "roblox-ts-template",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			},
			"$path": "models/Workspace",
			"Baseplate": {
				"$className": "Part",
				"$properties": {
					"Anchored": true,
					"Color": [0.38823, 0.37254, 0.38823],
					"Locked": true,
					"Position": [0, 0, 0],
					"Size": [50, 2, 50]
				}
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"TestService": {
			"$className": "TestService",
			"$properties": {
				"ExecuteWithStudioRun": true
			},
			"TS": {
				"$path": "test/run-tests.server.luau"
			}
		}
	}
}
